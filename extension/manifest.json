{
  "manifest_version": 3,
  "name": "Word Detector",
  "version": "1.0",
  "description": "Extension to detect specific words on web pages.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["extension.js"],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "48": "hello_extensions.png"
  }
}